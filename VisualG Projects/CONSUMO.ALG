Algoritmo "consumo"




Var

   distacia : inteiro
   consumomedio, combustivelGasto : real


Inicio

   escreva("Distancia percorrida: ")
   leia(distacia)
   escreva("Combustível gasto: ")
   leia(combustivelGasto)
   
   
   consumomedio <- distacia / combustivelGasto
   
   escreval("Consumo medio = ", consumomedio:6:3)



Fimalgoritmo